# Supabase (create project at https://supabase.com)
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Optional: Recipe parser API (deploy api/ to Vercel and set this URL)
EXPO_PUBLIC_PARSER_API_URL=

# Vision API for image-to-ingredients (gemini | openai)
EXPO_PUBLIC_VISION_PROVIDER=gemini
EXPO_PUBLIC_GEMINI_API_KEY=
EXPO_PUBLIC_OPENAI_API_KEY=

# Optional: YouTube recipe extraction (set in Vercel env, not in app .env)
# YOUTUBE_API_KEY=your-google-api-key

# Optional: Parse API vision fallback – when description/captions/comments have no ingredients,
# the API uses the video thumbnail + Gemini to detect ingredients from the frame (Vercel env only).
# GEMINI_API_KEY=your-gemini-api-key

# RevenueCat (create app at https://app.revenuecat.com)
# Use Test Store key when running in Expo Go (Apps and providers → Test configuration → Test Store API key)
EXPO_PUBLIC_REVENUECAT_API_KEY_TEST=your-test-store-api-key
EXPO_PUBLIC_REVENUECAT_API_KEY_IOS=your-ios-api-key
EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID=your-android-api-key
